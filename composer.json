{
    "name": "umpire274/var_dumper",
    "description": "A PHP library for printing PHP variables, arrays, objects and other complex structures in human readable format",
    "type": "library",
    "keywords": [
        "PHP",
        "var_dump",
        "var_export",
        "print_r",
        "udump"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "var_dumper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "var_dumper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Umpire274",
            "email": "umpire274@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "8.3",
        "hamcrest/hamcrest-php": "^2.0"
    }
}
